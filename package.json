{
  "name": "shogun-stealth-address",
  "version": "1.6.1",
  "description": "Stealth address functionality for Shogun SDK",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/browser/shogun-stealth-address.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:browser",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:browser": "webpack --config webpack.config.js",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "test": "jest",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "stealth",
    "address",
    "ethereum",
    "crypto",
    "privacy",
    "shogun"
  ],
  "author": "Scobru",
  "license": "MIT",
  "dependencies": {
    "@fluidkey/stealth-account-kit": "^1.1.0",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "ethers": "^6.13.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "shogun-bip44": "^1.6.0",
    "shogun-core": "^1.9.4",
    "stream-browserify": "^3.0.0"
  },
  "peerDependencies": {
    "gun": "^0.2020.1240",
    "shogun-core": "^1.5.15"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.27.9",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "@scure/bip32": "1.3.2"
  }
}
